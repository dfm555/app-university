<p id="notice"><%= notice %></p>

<h1>Listing Rols</h1>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @rols.each do |rol| %>
      <tr>
        <td><%= rol.name %></td>
        <td><%= link_to 'Show', rol %>
          <%= link_to 'Edit', edit_rol_path(rol) %>
          <%= link_to 'Destroy', rol, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rol', new_rol_path %>
